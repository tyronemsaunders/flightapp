{
  "dest" : {
    "build" : {
      "js" : {
        "site" : "build/app/site",
        "vendor" : "build/app/vendor"
      },
      "css" : {
        "core" : "build/assets/css",
        "site" : "build/assets/css/site",
        "vendor" : "build/assets/css/vendor"
      },
      "media" : "build/assets/media",
      "fonts" : "build/assets/fonts"
    },
    "prod" : {
      "js" : "bin/app",
      "css" : "bin/assets/css",
      "media" : "bin/assets/media",
      "fonts" : "bin/assets/fonts"
    },
    "src" : {
      "js" : {
        "site" : "src/app",
        "vendor" : "src/vendor-js"
      },
      "styles" : {
        "site" : "src/assets/styles/site",
        "vendor" : "src/assets/styles/vendor"
      },
      "media" : "src/assets/media",
      "fonts" : "src/assets/fonts"
    }
  },
  "src" : {
    "app" : {
      "js" : {
        "site" : [
          "src/app/**/!(*spec).js"
        ],
        "vendor" : [
          "src/vendor-js/**/*.js", 
          "src/vendor-js/*.js" 
        ]
      },
      "templates" : [
        "src/app/**/*.tpl.html",
        "src/app/*.tpl.html"
      ],
      "styles" : {
        "vendor" : [
            "src/assets/styles/vendor/**/*.scss", 
            "src/assets/styles/vendor/**/*.css",
            "!src/assets/styles/vendor/partials/**"
        ],
        "vendorPartials" :[
            "src/assets/styles/vendor/partials/**",
            "!src/assets/styles/vendor/partials/*.scss",
            "!src/assets/styles/vendor/partials/**/*.scss"
        ],
        "site" : [ 
            "src/assets/styles/site/**/*.scss",
            "src/assets/styles/site/*.scss",
            "src/assets/styles/site/**/*.css",
            "src/assets/styles/site/*.css",
            "src/app/**/*.scss", 
            "src/app/*.scss",
            "src/app/**/*.css", 
            "src/app/*.css"
        ]
      },
      "assets" : {
        "media" : [
          "src/assets/media/*.*",
          "src/assets/media/**/*.*"  
        ],
        "fonts" : [
          "src/assets/fonts/*.*(eot|svg|ttf|woff|woff2)",
          "src/assets/fonts/**/*.*(eot|svg|ttf|woff|woff2)"
        ]
      }
    },
    "vendor" : {
      "js" : [
        "vendor/angular/angular.js",
        "vendor/angular-animate/angular-animate.js",
        "vendor/angular-resource/angular-resource.js",
        "vendor/angular-aria/angular-aria.js",
        "vendor/angular-messages/angular-messages.js",
        "vendor/angular-sanitize/angular-sanitize.js",
        "vendor/angular-foundation-6/dist/angular-foundation.js",
        "vendor/angular-spinners/dist/angular-spinners.js",
        "vendor/angular-ui-router/release/angular-ui-router.js",
        "vendor/angular-ui-select/dist/select.js",
        "vendor/angularjs-slider/dist/rzslider.js",
        "vendor/what-input/what-input.js",
        "vendor/leaflet/dist/leaflet.js",
        "vendor/moment/moment.js",
        "vendor/moment-duration-format/lib/moment-duration-format.js",
        "vendor/Leaflet.GeometryUtil/dist/leaflet.geometryutil.js",
        "vendor/leaflet.markercluster/dist/leaflet.markercluster-src.js",
        "vendor/Leaflet.Snap/leaflet.snap.js"
      ],
      "templates" : [
        
      ],
      "styles" : [
      	"src/assets/styles/vendor/foundation-flex-hack.scss",
      	"vendor/font-awesome/scss/font-awesome.scss",
      	"src/assets/fonts/google-fonts.scss",
        "vendor/leaflet/dist/leaflet.css", 
        "vendor/angular-ui-select/dist/select.css",
        "vendor/angularjs-slider/dist/rzslider.scss",
        "vendor/leaflet.markercluster/dist/MarkerCluster.css"
      ],
      "stylePartials" : [
        "!vendor/font-awesome/scss/font-awesome.scss",
        "!vendor/foundation-sites/.github",
        "!vendor/foundation-sites/.github/**/*",
		"!vendor/foundation-sites/assets",
		"!vendor/foundation-sites/assets/**/*",
		"!vendor/foundation-sites/customizer",
		"!vendor/foundation-sites/customizer/**/*",
		"!vendor/foundation-sites/dist",
		"!vendor/foundation-sites/dist/**/*",
		"!vendor/foundation-sites/js",
		"!vendor/foundation-sites/js/**/*",
		"!vendor/foundation-sites/*",
		"vendor/font-awesome/scss/*.scss",
      	"vendor/foundation-sites/scss/*.scss",
        "vendor/foundation-sites/scss/**/*.scss",
		"vendor/foundation-sites/**/*"
      ],
      "assets" : {
        "images" : [
        	"vendor/leaflet/dist/images/*.*",
        	"vendor/Leaflet.Snap/*.png"
        ],
        "fonts" : [
        	"vendor/font-awesome/fonts/**"
        ]
      }
    }
  }
}